{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": true,
    "module": "commonjs",
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noEmit": true,
    "noEmitOnError": true,
    "allowJs": false,
    "target": "es2017",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2017"]
  }
}
